{
  "name": "FzGuardAPI",
  "version": "1.0.0",
  "description": "api que controla o FZguard portal, leitores fisicos e aplica√ßao movel",
  "author": "Moreira de Sousa",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copy-assets && npm run copy-keys",
    "copy-assets": "cp -r src/assets dist/",
    "copy-keys": "cp -r src/auth-keys dist/",
    "build-documentation": "npx compodoc -p tsconfig.json -d dist/docs --hideGenerator --silent",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "@nestjs/common": "^6.11.11",
    "@nestjs/core": "^6.11.11",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/terminus": "^7.0.1",
    "@types/socket.io": "^2.1.11",
    "@types/sqlite3": "^3.1.6",
    "activedirectory2": "^2.1.0",
    "bcrypt": "^3.0.8",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "mongoose-fuzzy-searching": "^2.0.2",
    "nest-csv-parser": "^2.0.4",
    "nodemailer": "^6.4.11",
    "pdfmake": "^0.1.68",
    "pm2": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.6.2",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.2.0",
    "swagger-ui-express": "^4.1.4",
    "tslint": "^6.1.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/testing": "^6.11.11",
    "@types/express": "^4.17.7",
    "@types/jest": "25.1.2",
    "@types/node": "^13.13.15",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
